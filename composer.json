{
	"name": "antonythorpe/silvershop-jsonresponse",
	"description": "SilverShop submodule that provides JSON responses for cart updates",
	"type": "silverstripe-vendormodule",
	"keywords": ["Silverstripe", "SilverShop", "ecommerce", "ajax", "json"],
	"license": "MIT",
	"require": {
		"silvershop/core": "^3.0",
        "silverstripe/vendor-plugin": "^1"
	},
    "autoload": {
        "psr-4": {
            "AntonyThorpe\\SilverShopJsonResponse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AntonyThorpe\\SilverShopJsonResponse\\Tests\\": "tests/"
        }
    },
	"extra": {
		"branch-alias": {
			"dev-master": "3.*-dev"
		}
	}
}
